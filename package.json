{
  "name": "marble",
  "description": "osu bot for local league event",
  "version": "0.2.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src",
    "start": "tsnd --rs --transpile-only -- src/Marble.ts",
    "start:dev": "tsnd --rs --inspect --transpile-only -- src/Marble.ts",
    "watch": "tsnd --rs --respawn --transpile-only src/Marble.ts",
    "watch:dev": "tsnd --rs --inspect --respawn --transpile-only -- src/Marble.ts"
  },
  "devDependencies": {
    "@types/node": "^16.4.10",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "eris": "^0.15.1",
    "ramune": "github:LavaDesu/Ramune#0.0.6",
    "slash-create": "^3.4.1"
  }
}
